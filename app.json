        "authDomain": "byulworks.firebaseapp.com",
        "projectId": "byulworks",
        "storageBucket": "byulworks.firebasestorage.app",
        "messagingSenderId": "2301088074",
        "appIdIos": "1:2301088074:ios:1d2d29ec5ca3b14bf5b1b3",
        "appIdAndroid": "1:2301088074:android:1d2d29ec5ca3b14bf5b1b3"
      }
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.byulsoft.worksrn",
