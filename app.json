{
  "expo": {
    "name": "ByulWorksRN",
    "slug": "ByulWorksRN",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "worksrn",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "extra": {
